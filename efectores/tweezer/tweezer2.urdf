<?xml version="1.0" ?>
<robot name="gripper">
  <link name="base_link"/>
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0.04 0 0"/>
      <inertia ixx="1e-5" ixy="0" ixz="0" iyy="1e-5" iyz="0" izz="1e-5"/>
    </inertial>

    <visual>
      <origin rpy="0 0 0" xyz="0.04 0 0"/>
      <geometry>
        <mesh filename="package://meshes/visual/tweezer_baselink.stl" scale="1000 1000 1000" />
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0.04 0 0"/>
      <geometry>
        <mesh filename="package://meshes/collision/tweezer_baselink.stl" scale="1000 1000 1000" />
      </geometry>
    </collision>

  </link>


  <link name="phalanx1"/>

    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0.04 0 0"/>
      <inertia ixx="1e-5" ixy="0" ixz="0" iyy="1e-5" iyz="0" izz="1e-5"/>
    </inertial>

    <visual>
      <origin rpy="0 0 0" xyz="0.04 0 0"/>
      <geometry>
        <mesh filename="package://meshes/visual/tweezer_patita.stl" scale="1000 1000 1000" />
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0.04 0 0"/>
      <geometry>
        <mesh filename="package://meshes/collision/tweezer_patita.stl" scale="1000 1000 1000"/>
      </geometry>
    </collision>

  </link>


  <link name="phalanx2"/>
      <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0.04 0 0"/>
      <inertia ixx="1e-5" ixy="0" ixz="0" iyy="1e-5" iyz="0" izz="1e-5"/>
    </inertial>

    <visual>
      <origin rpy="0 0 0" xyz="0.04 0 0"/>
      <geometry>
        <mesh filename="package://meshes/visual/tweezer_patita.stl" scale="1000 1000 1000"/>
      </geometry>
      <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0.04 0 0"/>
      <geometry>
        <mesh filename="package://meshes/collision/tweezer_patita.stl" scale="1000 1000 1000" />
      </geometry>
    </collision>
  </link>
  
  <joint name="joint1" type="prismatic">
    <parent link="base_link"/>
    <child link="phalanx1"/>
    <origin xyz="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.1" upper="0.1" effort="10" velocity="0.1"/>
  </joint>

  <joint name="joint2" type="prismatic">
    <parent link="phalanx1"/>
    <child link="phalanx2"/>
    <origin xyz="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="-0.1" upper="0.1" effort="10" velocity="0.1"/>
  </joint>
  
</robot>

